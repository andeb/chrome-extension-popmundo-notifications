{
  "name": "Popmundo Notification",
  "description": "An unofficial app to show the popmundo notifications.",
  "version": "0.3",
  "manifest_version": 2,
  "permissions": [
    "http://*.popmundo.com/*",
    "webRequest",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.popmundo.com/*"
      ],
      "js": [
        "content_scripts.js"
      ]
    }
  ],
  "icons": {
    "16": "images/p5757.png",
    "128": "images/p5757.png",
    "_comment" : "TODO Fix pictures"
  }
}